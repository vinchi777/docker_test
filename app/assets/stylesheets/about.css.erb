.content-space {
  padding-top: 20px;
  padding-bottom: 20px;
}
#about .banner {
  background: url("<%= asset_path 'about2.jpg'%>") no-repeat center -70px fixed;
  background-size: cover;
  height: 400px;
  position: relative;
  -webkit-background-size: cover;
  -moz-background-size: cover;
  -o-background-size: cover;
}
#about .banner .container .wrap {
  bottom: 22px;
  color: #fff;
  position: absolute;
}
#about .banner .container .wrap h1,
#about .banner .container .wrap h4 {
  margin: 0px;
}
#about .banner .container .wrap h4 {
  font-family: 'Signika Light';
}
#about > .container {
  padding-top: 20px;
  padding-bottom: 20px;
}
#about article {
  padding-top: 20px;
  padding-bottom: 20px;
}
#about .history > .row {
  padding: 5px 0px;
}
#about .history .rating {
  text-align: right;
}
#about .history .rating .percent {
  color: #555;
  vertical-align: top;
}
#about .history .rating .bar {
  display: inline-block;
  height: 14px;
  margin: 5px 0px 0px 5px;
}
#about .history .rating .bar1 {
  background-color: #edca66;
  width: 55%;
}
#about .history .rating .bar2 {
  background-color: #e8ba38;
  width: 65%;
}
#about .history .rating .bar3 {
  background-color: #d4a419;
  width: 75%;
}
