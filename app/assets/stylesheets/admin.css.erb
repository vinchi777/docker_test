/***
 Site's variables
*****/
.navbar.header .container.admin {
  width: 98%;
}
body {
  height: 100%;
}
.admin-container {
  position: relative;
  float: left;
  width: 100%;
  min-height: 550px;
}
.admin-nav {
  background: #4C4C4C;
  padding: 0;
  position: absolute;
  height: 100%;
}
.admin-nav ul.admin-nav > li {
  float: left;
  width: 100%;
}
.admin-nav ul.admin-nav > li > a {
  background: #3E3E3E;
  border-left: 3px solid #2980B9;
  border-radius: 0;
  color: #B2B2B2;
  font-family: "Signika Semibold";
  font-size: 16px;
  float: left;
  width: 100%;
}
.admin-nav ul.admin-nav > li.active > a,
.admin-nav ul.admin-nav > li.active > a:hover,
.admin-nav ul.admin-nav > li > a:hover {
  background: #323232;
}
.admin-nav ul.admin-nav > li > a .ico {
  width: 22px;
  float: left;
  margin-right: 10px;
}
.admin-nav ul.admin-nav > li > a > .title {
  margin-left: 32px;
}
.admin-nav ul.student {
  color: #CCC;
  font-family: 'Signika Semibold';
  float: left;
  width: 100%;
}
.admin-nav ul.student a {
  color: #CCC;
}
.admin-nav ul.student > li > a {
  padding: 8px 15px;
}
.admin-nav ul.student li,
.admin-nav ul.student li a:hover,
.admin-nav ul.student li a:focus,
.admin-nav ul.student li.active a,
.admin-nav ul.student li.active > a:hover,
.admin-nav ul.student li.active > a:focus {
  background: #585858;
}
.admin-nav ul.student li.active {
  border-right: 3px solid #DF5A49;
}
.admin-nav ul.student li.student {
  height: 64px;
  padding: 9px 10px;
  box-shadow: 0 2px 1px rgba(0, 0, 0, 0.2) inset;
}
.admin-nav ul.student li.student img {
  height: 48px;
  width: 48px;
}
.admin-nav ul.student li.student .pic {
  width: 60px;
  float: left;
}
.admin-nav ul.student li.attr {
  font-size: 16px;
  float: left;
  width: 100%;
}
.admin-nav ul.student li.attr > a {
  float: left;
}
.admin-nav ul.student li.attr > a:hover {
  color: #EEE;
}
.admin-nav ul.student li.attr img {
  width: 24px;
}
.admin-nav ul.student li.attr .ico {
  width: 35px;
  float: left;
}
.admin-nav ul.student li.attr .title {
  padding-top: 1px;
  margin-left: 35px;
}
.admin-nav li.review-seasons a {
  padding-top: 8px;
}
.admin-nav li.user img {
  height: 18px;
  margin-top: -2px;
}
.admin-content {
  padding: 30px;
}
.admin-content header h2 {
  font-family: 'Signika Semibold';
  font-size: 20px;
  color: #4C4C4C;
  margin-bottom: 12px;
}
.admin-content header .search input[type='search'] {
  width: 100%;
  background: url("<%= image_path 'search.svg' %>") no-repeat scroll 7px 6px;
  background-size: 17px;
  padding-left: 33px;
}
.count.listing {
  font-size: 14px;
  margin-top: 5px;
}
.listing.panel {
  margin-top: 7px;
}
.listing.panel .empty-list {
  padding-top: 10px;
}
.listing.panel .no-result {
  padding: 7px;
  height: 42px;
  color: #555;
}
.listing.panel .no-result .fa {
  font-size: 23px;
  width: 27px;
  margin-top: 3px;
  float: left;
}
.listing.panel .no-result div {
  padding-top: 3px;
}
.listing.panel > .item.row {
  padding-top: 10px;
  padding-bottom: 10px;
}
.listing.panel > .item.row:hover {
  background: #E5E5E5 !important;
}
.listing.panel > .item.row:hover .actions {
  color: #919191;
}
.listing.panel > .item.row:nth-child(odd) {
  background: #F8F8F8;
}
.listing.panel > .item.row img.profile {
  width: 52px;
  height: 52px;
}
.listing.panel > .item.row .info {
  line-height: 17px;
}
.listing.panel > .item.row .actions {
  color: #E5E5E5;
  text-align: right;
}
div.pagination {
  clear: both;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
}
div.pagination .pagination {
  margin: 0;
}
div.pagination .pagination > li:first-child > a,
div.pagination .pagination > li:last-child > a {
  font-size: 24px;
  padding-top: 0;
  padding-bottom: 0;
  padding-left: 6px;
  width: 32px;
}
div.pagination .pagination > li:nth-child(2) > a,
div.pagination .pagination > li:nth-last-child(2) > a {
  padding-left: 9px;
  padding-right: 0;
  padding-top: 6px;
  width: 32px;
  height: 32px;
}
div.pagination .pagination > li > a {
  border: none;
  border-radius: 100%;
  margin-left: 5px;
  margin-right: 5px;
  color: #000000;
  height: 32px;
  padding-top: 5px;
  font-weight: bold;
}
div.pagination .pagination > li > a:hover {
  background: #fed24e;
}
div.pagination .pagination > li.disabled > a {
  color: #DDD;
}
div.pagination .pagination > li.disabled > a:hover {
  background: none;
}
div.pagination .pagination > li.active > a {
  background: #fed24e;
  width: 32px;
}
.loading-list {
  padding-top: 150px;
  text-align: center;
  height: 312px;
}
footer.admin {
  float: left;
  width: 100%;
  background-color: #333333;
  color: #E6E6E6;
  text-align: center;
  height: 70px;
  font-size: 15px;
  padding-top: 16px;
  line-height: 20px;
}
footer.admin a {
  color: #E6E6E6;
}
.modal .error ul {
  padding-left: 20px;
}